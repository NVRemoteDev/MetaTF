                <%}%>
              <%}%>
            <%}%>
          <%}%>
        <% while(lastFound < bpslots) { %>
          <div class="span2 normal backgroundmodifiers normal bpitem"></div>
          <% usedASlot++; %>
          <% if (usedASlot % 50 === 0) { %>
            <% if(lastFound + 1 !== bpslots) {%>
              <p>Page <%= ++currentPage %></p>
            <%}
          }
          lastFound++;
        }%>
        <% results = null; %>
        <% console.log("Finished rendering."); %>
      <% } else { %>
