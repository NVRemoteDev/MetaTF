                      <p>Level: <%= results[i].level %> <%= results[i].item_type_name %></p>
                    </div>
                    <% lastFound = results[i].bpposition; %>
                  <% break; %>
                  <% case 1: %> <div class="span2 genuine backgroundmodifiers normal bpitem" style="background-image:url('<%= results[i].image_url %>');background-size: contain;">
                    <% if(results[i].quantity !== undefined && results[i].quantity !== 1) { %><%= results[i].quantity %><% } %>
                    <div class="iteminfo text-center">
                      <p class="itemname genuinefont">Genuine <%= results[i].name %></p>
                      <p>Level: <%= results[i].level %> <%= results[i].item_type_name %></p>
