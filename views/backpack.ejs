<% include header %>
    <h1>Steam Results for <% if(id) { %><%= id %> <% } else { %><%= user %><% } %></h1>
    <% if (results.length) { %>
      <p>
        Backpack Used/Total Slots: Slots taken: <%= results.length %>/<%= bpslots %>
      </p>
      <ul>
        <% for (var i = 0; i < results.length; i++) { %>
          <li><%= results[i].defindex %> - <%= results[i].quantity %></li>
        <% } %>
      </ul>
    <% } else { %>
      <p>No results</p>
    <% } %>
<% include footer %>